{
  "name": "@mapd/mapdc",
  "version": "0.14.0",
  "license": "Apache-2.0",
  "copyright": "2014",
  "description": "A multi-dimensional charting library built to work natively with crossfilter and rendered using d3.js.",
  "keywords": [
    "visualization",
    "svg",
    "animation",
    "canvas",
    "chart",
    "dimensional",
    "crossfilter",
    "d3"
  ],
  "homepage": "http://mapd.com",
  "bugs": "https://github.com/map-d/mapdc.js/issues",
  "author": {
    "name": "MapD Technologies",
    "url": "http://mapd.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/map-d/mapdc.js.git"
  },
  "main": "src/index.js",
  "scripts": {
    "build": "bash scripts/build.sh",
    "build:css": "./node_modules/.bin/node-sass --include-path scss scss/chart.scss chart.css",
    "build:example": "npm run build && bash scripts/copy-example-libs.sh",
    "clean": "bash scripts/clean.sh",
    "docs": "./node_modules/.bin/esdoc -c .esdoc.json && open docs/index.html",
    "lint": "./node_modules/.bin/eslint",
    "shjs": "node --harmony_destructuring ./node_modules/.bin/shjs",
    "start": "npm run build:example && node ./node_modules/http-server/bin/http-server -p 8081 -o -c-1",
    "test": "npm run shjs ./scripts/run-unit-tests.js",
    "test:bump": "npm run shjs ./scripts/check-coverage.js -- --bump",
    "test:check-cover": "npm run shjs ./scripts/check-coverage.js",
    "test:cover": "COVER=true npm run shjs ./scripts/run-unit-tests.js",
    "webpack": "node ./node_modules/webpack/bin/webpack.js"
  },
  "dependencies": {
    "@mapd/connector": "ssh://git@github.com/mapd/mapd-connector-os-testing.git",
    "@mapd/crossfilter": "ssh://git@github.com/mapd/mapd-crossfilter-os-testing.git",
    "d3": "^3.5.17",
    "deep-equal": "1.0.1",
    "earcut": "^2.1.1",
    "mapbox-gl-draw": "https://github.com/mapd/mapbox-gl-draw/tarball/jon/upgrade",
    "mapbox-gl-js": "https://github.com/mapd/mapbox-gl-js/tarball/9c04de6949fe498c8c79f5c0627dfd6d6321f307",
    "mapd-draw": "ssh://git@github.com/mapd/mapd-draw.git#v1.1.0",
    "moment": "2.13.0",
    "ramda": "0.21.0",
    "simplify-js": "^1.2.1"
  },
  "devDependencies": {
    "atob": "^2.0.3",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.1",
    "babel-loader": "6.2.4",
    "babel-plugin-ramda": "1.1.1",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.4.3",
    "chai": "^3.5.0",
    "chai-spies": "^0.7.1",
    "css-loader": "0.22.0",
    "eslint": "^3.0.1",
    "eslint-config-airbnb": "^3.1.0",
    "eslint-plugin-import": "1.16.0",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "0.8.0",
    "http-server": "^0.9.0",
    "istanbul": "1.0.0-alpha.2",
    "jscs": "2.11.0",
    "jsdom": "3.1.2",
    "lodash": "4.3.0",
    "mocha": "^2.5.3",
    "mocha-jenkins-reporter": "^0.1.9",
    "node-sass": "^3.4.2",
    "proxyquire": "1.7.4",
    "sass-loader": "3.1.2",
    "shelljs": "0.7.5",
    "style-loader": "0.13.0",
    "webpack": "^1.13.1"
  },
  "npmName": "mapdc",
  "npmFileMap": [
    {
      "basePath": "/",
      "files": [
        "dc.css",
        "dc.min.css",
        "dc.min.js",
        "dc.min.js.map",
        "dc.js",
        "dc.js.map"
      ]
    }
  ]
}
